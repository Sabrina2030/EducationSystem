@model List<MateriaViewModel>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre Materias:</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>
